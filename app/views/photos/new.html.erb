<%= link_to "New Photo", new_photo_path, class: "btn btn-primary" %>

<% @photo.each do |photo| %> 
 <div class="card photo" >
   <div class="card-header">
     <%= photo.username %>
   </div>
   <%= image_tag photo.url , class: "card-img-top" %>
   <div class="card-block">
     <p class="card-text">
       <%= photo.caption %>
     </p>
   </div>
 </div>
<% end %> 