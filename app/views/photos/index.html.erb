<h1>Photos#index</h1>
<p>Find me in app/views/photos/index.html.erb</p>


  
 <% @photo.each do |photo| %>
    <div class ="card">
      <div class="card-header"> 
        <%= photo.username  %>
      </div>
     <%= image_tag photo.url, class: "card-img-top", alt:"photo" %>
     <div class="card-block">
      <p class="card-text">
        <%= photo.caption %>
      </p>
     </div>
    </div>
 <% end %>
   

